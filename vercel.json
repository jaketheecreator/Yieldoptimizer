{
  "version": 2,
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "api/**/*": {
      "memory": 1024,
      "maxDuration": 10
    },
    "pages/**/*": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "cleanUrls": true,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 